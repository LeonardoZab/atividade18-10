<h1>Consulta</h1>
<br>
<form action="/consultar" method="post">
    <label for="">Digite o nome da Atividade:</label>
    <input type="text" name="nome">
    <button type="submit">Consulta</button>
</form>
<br>
{{#if msg}}
    <p>{{msg}}</p>
{{/if}}
{{#if msg2}}
    <p>{{msg2}}</p>
{{/if}}
<br>
{{#if valores}}
<table>
    <thead>
        <th>Código</th>
        <th>Nome</th>
        <th>Descrição</th>
    </thead>
    <tbody>
        <tr>
            <td>{{valores.id}}</td>
            <td>{{valores.Nome}}</td>
            <td>{{valores.Descricao}}</td>
        </tr>
    </tbody>
</table>
{{/if}}